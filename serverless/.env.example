# pg_deeplake Serverless Deployment Configuration
# Copy to .env and fill in your values:  cp .env.example .env

# AWS Credentials (required for S3 storage)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=

# DeepLake S3 storage path (each deployment needs a unique path)
DEEPLAKE_ROOT_PATH=s3://your-bucket/your-prefix/

# Catalog sync interval (ms) â€” how often instances poll S3 for catalog changes
DEEPLAKE_SYNC_INTERVAL_MS=2000

# Memory limit for pg_deeplake (0 = unlimited)
PG_DEEPLAKE_MEMORY_LIMIT_MB=0

# Load balancer profile: "haproxy" (default) or "supavisor"
COMPOSE_PROFILES=haproxy

# Optional: enable HAProxy stats UI by setting both values
HAPROXY_STATS_USER=
HAPROXY_STATS_PASSWORD=

# PostgreSQL
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=postgres
